Imports System.Configuration
Imports System.Data.SqlClient
Imports DataAccessHandler
Imports Telerik.WinControls.UI
Public Class frmTested
    Private Sub frmTested_Load(sender As Object, e As EventArgs) Handles Me.Load
        Me.CopyColumnSchema(Me.radGridView1, Me.radGridView2)
    End Sub
    Private Sub CopySelectedRows(ByVal sourceGrid As RadGridView, ByVal destinationGrid As RadGridView)
        For i As Integer = 0 To sourceGrid.SelectedRows.Count - 1
            Dim values As Object() = New Object(sourceGrid.ColumnCount - 1) {}

            For c As Integer = 0 To sourceGrid.SelectedRows(i).Cells.Count - 1
                values(c) = sourceGrid.SelectedRows(i).Cells(c).Value
            Next

            destinationGrid.Rows.Add(values)
        Next
    End Sub
